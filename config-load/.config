# Global information
 title = "TOML example"

 # Application Information
[app]
         author = "Splash"
    organization = "Mafool"
         mark = "The first line\nThe second line." # wrap
         release = 2020-05-27T07:32:00Z # time

 # Database configuration
[mysql]
    server = "192.168.1.1"
         ports = [8001, 8001, 8002] # array
    connection_max = 5000
    enabled = true

 # Redis master-slave # dictionary object
[redis]
    [redis.master]
        host = "10.0.0.1"
        port = 6379
    [redis.slave]
        host = "10.0.0.1"
        port = 6380

 # Two-dimensional array 
[releases]
release = ["dev", "test", "stage", "prod"]
tags = [["dev", "stage", "prod"],[2.2, 2.1]]


 # #Object Nesting
[company]
         name = "xx Technology"
[company.detail]
    type = "game"
         addr = "Beijing Chaoyang"
    icp = "030173"